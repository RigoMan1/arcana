{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "singleAttributePerLine": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "vueIndentScriptAndStyle": false,
  "printWidth": 90,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "strict",
  "pluginSearchDirs": false,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "prettier-plugin-classnames"
  ]
}